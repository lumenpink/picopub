{
  "name": "indiekit",
  "version": "1.0.0",
  "description": "Indiekit server for https://lumen.pink",
  "main": "index.js",
  "scripts": {
    "start": "indiekit serve"
  },
  "keywords": [
    "indiekit",
    "indieweb"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "20"
  },
  "type": "module",
  "indiekit": {
    "plugins": [
      "@indiekit/preset-hugo",
      "@indiekit/store-github",
      "@indiekit/syndicator-mastodon"
    ],
    "publication": {
      "me": "https://lumen.pink"
    },
    "@indiekit/preset-hugo": {
      "frontMatterFormat": "yaml"
    },
    "@indiekit/store-github": {
      "user": "lumenpink",
      "repo": "site",
      "branch": "main"
    },
    "@indiekit/syndicator-mastodon": {
      "url": "https://colorid.es",
      "user": "lumen"
    }
  },
  "dependencies": {
    "@indiekit/indiekit": "^1.0.0-beta.7",
    "@indiekit/preset-hugo": "^1.0.0-beta.6",
    "@indiekit/store-github": "^1.0.0-beta.6",
    "@indiekit/syndicator-mastodon": "^1.0.0-beta.6"
  }
}
